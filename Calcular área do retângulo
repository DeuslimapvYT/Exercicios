    <h1>Calcular Ret창ngulo</h1>
    <form name="f1rm">
        <p>Comprimento do ret창ngulo</p>
            <input type="text" name="comp">
        <p>Altura do ret창ngulo</p>
            <input type="text" name="alt">
            <br>
            <br>
        <input type="button" onclick="inicio()" value="INICIAR">
        <p>Resposta</p>
            <input type="text" name="resp" disable="true">
